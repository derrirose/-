#include <stdio.h>
#include <string.h>
int main() {
    char str[1000];
    int len = 0;
    int i = 0;
    while (scanf("%s",str) != EOF) { // 注意 while 处理多个 case
       
     
    }
    i = strlen(str);
     for(int j = 0;j< i;j++)
      {
        if(str[j]!=' '){
            len++;
        }
        else{
            len = 0;
        }
      }
    printf("%d",len);
    return 0;
}
